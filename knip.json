{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "project": [
    "{src,examples}/**/*.{ts,js}",
    "*.{config,cfg}.{ts,js}",
    "reliverse.ts"
  ],
  "entry": [
    "src/main.ts",
    "*.{config,cfg}.{ts,js}",
    "examples/launcher.ts",
    "examples/src/other/**/*.ts"
  ],
  "ignoreExportsUsedInFile": true,
  "ignoreBinaries": ["relidler"],
  "ignoreDependencies": ["@reliverse/relidler-cfg"]
}
