{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "eslint.config.js",
    "build.config.ts",
    "build.publish.ts",
    "addons/**/*.ts",
    "examples/**/*.ts",
    "src/main.ts"
  ],
  "project": [
    "eslint.config.js",
    "build.config.ts",
    "build.publish.ts",
    "addons/**/*.ts",
    "src/**/*.ts",
    "examples/**/*.ts"
  ],
  "ignoreExportsUsedInFile": true,
  "ignore": ["tests-runtime/", "dist-jsr/", "dist-npm/", "dist-libs/"],
  "ignoreDependencies": ["tinyglobby", "confbox", "destr", "pkg-types"]
}
