{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "eslint.config.js",
    "build.config.ts",
    "build.publish.ts",
    "addons/**/*.ts",
    "src/main.ts",
    "examples/launcher.ts",
    "examples/src/other/cmds/build.ts",
    "examples/src/other/cmds/debug.ts",
    "examples/src/other/cmds/deploy.ts",
    "examples/src/other/args-a.ts",
